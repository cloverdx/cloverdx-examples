	$out.0.dateField = today();
	$out.0.smallDecimalField = 1.24d;
	$out.0.largeDecimalField = 1234567890123456789012.123456789d;
	$out.0.integerField = 5;
	$out.0.longField = 10;
	$out.0.numberField = 3.14159265358;
	$out.0.stringField = "hello";
	$out.0.variantField = { "x" -> "y" };
	
	return ALL;
}
]]></attr>
